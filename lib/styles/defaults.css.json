{
	"h1, h2, h3, h4, h5" : {
		"display" : "block",
		"margin" : "0",
		"padding" : "0",
		"font-family" : "font-family-2"
	},
	"h1" : {
		"font-size" : "26px",
		"line-height" : "30px"
	},
	"h2" : {
		"font-size" : "21px",
		"line-height" : "25px"
	},
	"img" : {
		"display" : "block",
		"max-width" : "100%"
	},
	"html, body" : {
		"padding" : "0",
		"margin" : "0"
	},
	"body" : {
		"font-family" : "font-family-1",
		"color" : "#464646"
	},
	"p": {
		"font-family": "font-family-1"
	},
	"td": {
		"font-family": "font-family-1"
	},
	".container" : {
		"width" : "600px",
		"margin" : "0 auto"
	},
	".align-center" : {
		"text-align" : "center"
	},
	".align-right": {
		"text-align" :"right"
	},
	".button-wrapper" : {
		"background-color": "#505050",
		"border": "solid 1px #353535",
		"border-radius": "5px"
	},
	".button-outer": {
		"color" : "#FFFFFF" ,
		"font-family" : "Helvetica, Arial, sans-serif" ,
		"font-size" : "16px" ,
		"font-weight" : "bold" ,
		"letter-spacing" : "-.5px" 
	},
	".button" : {
		"color":"#FFFFFF",
		"text-decoration": "none",
		"line-height" : "150%" ,
		"padding-top" : "15px" ,
		"padding-right" : "30px" ,
		"padding-bottom" : "15px" ,
		"padding-left" : "30px",
		"display": "block"
	}
}
