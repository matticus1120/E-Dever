{
	"h1, h2, h3, h4, h5" : {
		"display" : "block",
		"margin" : "0",
		"padding" : "0",
		"font-family" : "font-family-2"
	},
	"h1" : {
		"font-size" : "26px",
		"line-height" : "30px"
	},
	"h2" : {
		"font-size" : "21px",
		"line-height" : "25px"
	},
	"img" : {
		"display" : "block",
		"max-width" : "100%"
	},
	"html, body" : {
		"padding" : "0",
		"margin" : "0"
	},
	"body" : {
		"font-family" : "font-family-1",
		"color" : "#464646"
	},
	"p": {
		"font-family": "font-family-1"
	},
	"td": {
		"font-family": "font-family-1"
	},
	".container" : {
		"width" : "600px",
		"margin" : "0 auto"
	},
	".align-center" : {
		"text-align" : "center"
	},
	".align-right": {
		"text-align" :"right"
	},
	".button" : {
		"font-size" : "25px",
		"color" : "#69c9ba",
		"text-decoration" : "none",
		"text-transform" : "uppercase",
		"-webkit-border-radius" : "0",
		"-moz-border-radius" : "0",
		"border-radius" : "0",
		"padding-top" : "2px",
		"padding-right" : "23px",
		"padding-bottom" : "3px",
		"padding-left" : "23px",
		"border" : "1px solid #69c9ba",
		"font-weight" : "300"
	}
}
